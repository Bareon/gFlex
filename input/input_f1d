; All units are SI

[mode]
model=flexure ; "flexure" or "PrattAiry"
dimension=1 ; 1 or 2 dimensions
method=FD ; Solution method: FD, FFT, SPA, SPA_NG

[parameter]
YoungsModulus=1.0E11
PoissonsRatio=0.25
GravAccel=9.8
MantleDensity=3300
InfillMaterialDensity=0
ElasticThickness=15000
; 30000
; 50000 ; Used for FFT, SPA, and SPA_NG solutions 

[input]
Loads=q0_sample/1D/central_block.txt ; q0_sample/1D/l_block.txt ; lr_blocks_100m_500cell.txt ; 
ElasticThickness=
;Te_sample/1D/8km_20km_ramp.txt ; Te_sample/1D/10km_const.txt ; 

[output]
DeflectionOut=
; tmpout.txt
; Plot choices are q0, w, both, or (for 1D) combo
Plot=combo

[numerical]
GridSpacing_x=6000
BoundaryCondition_West=0Moment0Shear
BoundaryCondition_East=Dirichlet0
; Solver can be direct or iterative; currently only works in 2D
; (Direct only in 1D, no matter what is written here)
Solver=direct
; Iterates until this is the difference between two subsequent iterations
; Set as 0 if you don't want to iterate
convergence=1E-3

[verbosity]
Verbose=False
Debug=False
