; All units are SI

[mode]
model=flexure ; "flexure" or "PrattAiry"
dimension=1 ; 1 or 2 dimensions
method=FD ; Solution method: FD, FFT, SPA, SPA_NG

[parameter]
YoungsModulus=1.0E11
PoissonsRatio=0.25
GravAccel=9.8
MantleDensity=3300
InfillMaterialDensity=1000
ElasticThickness=10000 ; Used for FFT and SPA solutions

[input]
Loads=q0_sample/1D/waterblock.txt
ElasticThickness=

[output]
DeflectionOut=tmpout.txt
Plot=combo

[numerical]
GridSpacing=1000
; Iterates until this is the difference between two subsequent iterations
; Set as 0 if you don't want to iterate
convergence=1E-3
