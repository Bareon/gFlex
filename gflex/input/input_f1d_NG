; All units are SI

[mode]
model=flexure ; "flexure" or "PrattAiry"
dimension=1 ; 1 or 2 dimensions
method=FD ; Solution method: FD, FFT, SPA, SPA_NG

[parameter]
YoungsModulus=1.0E11
PoissonsRatio=0.25
GravAccel=9.8
MantleDensity=3300
InfillMaterialDensity=0
ElasticThickness=15000
; 30000
; 50000 ; Used for FFT, SPA, and SPA_NG solutions 

[input]
Loads=q0_sample/1D/xy
ElasticThickness=
;Te_sample/1D/8km_20km_ramp.txt ; Te_sample/1D/10km_const.txt ; 

[output]
DeflectionOut=
; tmpout.txt
; Plot choices are q0, w, both, or (for 1D) combo
Plot=combo

[numerical]
GridSpacing_x=6000
BoundaryCondition_West=0Moment0Shear
BoundaryCondition_East=0Slope0Shear
; Solver can be direct or iterative
Solver=direct
; Iterates until this is the difference between two subsequent iterations
; (here given in meters)
; Function defaults will not be chosen, but are 1E-5, if you wish to set
; that here.
ConvergenceTolerance=0.001

[verbosity]
Verbose=False
Debug=False
