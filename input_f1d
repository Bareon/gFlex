; All units are SI

[mode]
model=flexure ; "flexure" or "prattairy"
dimension=1 ; 1 or 2 dimensions
method=GF ; Solution method: FD, FFT, GF

[parameter]
YoungsModulus=1.0E11
PoissonsRatio=0.25
GravAccel=9.8
MantleDensity=3300
InfillMaterialDensity=0
ElasticThickness=10000 ; Used for FFT and GF solutions

[input]
Loads=q0_sample/1D/central_block.txt
ElasticThickness=Te_sample/1D/10km_const.txt

[output]
ElasticThicknessOut=path3
LoadsOut=path4

[numerical]
GridSpacing=20000
; Iterates until this is the difference between two subsequent iterations
; Set as 0 if you don't want to iterate
convergence=1E-3
